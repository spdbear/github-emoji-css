{
  "name": "github-thumbdown-improve",
  "author": "spdbear",
  "description": "improve github emoji :-1:",
  "version": "1.1.0",
  "manifest_version": 2,
  "web_accessible_resources": ["*"],
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "css": ["style.css"],
      "js": ["browser-polyfill.min.js", "script.js"]
    }
  ],
  "browser_action": {
    "default_icon": {},
    "default_title": "github-thumbdown-improve",
    "default_popup": "popup.html"
  }
}
